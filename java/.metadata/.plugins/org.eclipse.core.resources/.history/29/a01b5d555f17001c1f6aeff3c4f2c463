package Interface08.Sample;

public interface RemoteControl {

	
	int MAX_VOLUME =10;
	int MIN_VOLUME = 0;
	
	void trunOn();
	void turnOff();
	void setVolume(int volume);
	
	
	default void setMute (boolean mute ) {
		if(mute) {System.out.println("무음처리합니다. ");}
		else System.out.println("무음 해제합니다. ");
	}
	
	
	
	
	
	
}
