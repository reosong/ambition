package array_05.ㄱㄷㄴㅅ;

public class TwoArray {

	public static void main(String[] args) {
		int[][] arr= new int[2][3];
		
		arr[0][0]=10;
		arr[0][1]=20;
		arr[0][2]= 30;
		arr[1][0]=40;
		arr[1][1]=50;
		arr[1][2]= 60;
		
		System.out.println("행의갯수 "+arr.length);
		System.out.println("열의 갯수 "+ arr[0].length);
		
		
		
		
		
	}

}
